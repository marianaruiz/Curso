package curso.projecto4.business;

import curso.ejercicio7.MaximoException;

public class sinStockExeption extends Exception{
    private String tipo;
    private int disponibles;
     
    
    public sinStockExeption(String tipo, int disponibles) {
        super();
        this.tipo = tipo;
        this.disponibles = disponibles;
    }
    throw new MaximoException("Llego a su maximo",MAXIMO);

    public String getTipo() {
        return tipo;
    }


    public void setTipo(String tipo) {
        this.tipo = tipo;
    }


    public int getDisponibles() {
        return disponibles;
    }


    public void setDisponibles(int disponibles) {
        this.disponibles = disponibles;
    }


    /**
     * 
     */
    private static final long serialVersionUID = 1L;

    
}
