package curso.ejemplo5;

public class Disco extends Coleccionable{
    private int pistas;
    
    public Disco(int identificador, int edicion,int pistas) {
        super(identificador, edicion);
        this.pistas=pistas;
    }
    public int getPistas() {
        return pistas;
    }

    public void setPistas(int pistas) {
        this.pistas = pistas;
    }
    public String imprimir() {
        return "identificador=" + getIdentificador() + "\nedicion=" + getEdicion()+"\npistas="+pistas;
    }
    
}
