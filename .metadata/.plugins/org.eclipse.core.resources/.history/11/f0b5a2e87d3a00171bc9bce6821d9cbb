package curso.ejemplo6;

import curso.ejemplo5.Coleccionable;

public class Coleccionador {
    Coleccionable[] coleccionable;
    
    public int ContarLibros(){
        int contador=0;
        for (Coleccionable coleccionador : coleccionable) {
        }
        return contador;
    }
    public int ContarDiscos(){
        int contador=0;
        for (Coleccionable coleccionador : coleccionable) {
        }
        return contador;
    }
    public int ContarTarjetas(){
        int contador=0;
        for (Coleccionable coleccionador : coleccionable) {
        }
        return contador;
    }
    public void agregarElemento(Coleccionable nuevoColeccionador){
        coleccionable[coleccionable.length+1]=nuevoColeccionador;
    }
    
}
