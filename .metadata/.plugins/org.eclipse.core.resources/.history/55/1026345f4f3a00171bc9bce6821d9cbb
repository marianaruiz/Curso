package curso.ejemplo4;

public class Calculadora {
    private int operandoUno;
    private int operandoDos;
    
    public Calculadora() {
        this(5, 5);
    }

    public Calculadora(int operandoUno, int operandoDos) {
        super();
        this.operandoUno = operandoUno;
        this.operandoDos = operandoDos;
    }
    
     
}
