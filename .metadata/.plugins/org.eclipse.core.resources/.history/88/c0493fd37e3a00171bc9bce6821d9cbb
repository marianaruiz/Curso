package curso.ejemplo5;

public class Libro extends Coleccionable {
    private String autor;
    
    public Libro(int identificador, int edicion,int autor) {
        super(identificador, edicion);
        // TODO Auto-generated constructor stub
    }
    
    public String getAutor() {
        return autor;
    }

    public void setAutor(String autor) {
        this.autor = autor;
    }
    public String imprimir() {
        return "identificador=" + getIdentificador() + "\nedicion=" + getEdicion()+"\nautor="+autor;
    }
}
